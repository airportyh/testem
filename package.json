  "devDependencies": {
    "mocha": "*",
    "sinon": "*",
    "async": "*",
    "chai": "*",
    "sinon-chai": "*"
  },
  "bin": {
    "testem": "./testem.js"
  },
  "bundleDependencies": [
    "tap"
  ],
  "optionalDependencies": {}
}
